  // 弹药计量样式，可选值：”normal“(显示实际值)、”percent“(显示百分比)，默认为”normal“
  "ammo_count_style": "percent",
  "animation": "trisdyna:cms92",
  "use_default_animation": "rifle",
  "third_person_animation": "default",
  // 显示准星，默认情况下瞄准会隐藏准星，将其设置为 true 则不会
  "show_crosshair": false,
  "transform": {
    "scale": {
      "thirdperson": [0.6, 0.6, 0.6],
      "ground": [0.6, 0.6, 0.6],
      "fixed": [1.2, 1.2, 1.2]
    }
  },
  "muzzle_flash": {
    "texture": "trisdyna:flash/tris_muzzle_flash",
    "scale": 0.6
  },
